{
  "name": "picwords",
  "description": "単語をストレスなく覚えるためのプラグイン",
  "manifest_version": 2,
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "登録",
    "default_popup": "popup.html"
  },
  "content_scripts":[{
    "matches": ["http://*/*"],
    "js": ["jquery.min.js", "picwords.js"],
    "run_at": "document_end"
  }],
  "icons": {
    "48": "icon.png"
  }
}
