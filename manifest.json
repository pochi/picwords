{
  "name": "picwords",
  "description": "単語を記憶するためのプラグイン",
  "manifest_version": 2,
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Picwords",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "scripts": ["jquery.min.js", "script.js"],
    "persistent": false
  },
  "content_scripts":[{
    "matches": ["http://ejje.weblio.jp/*"],
    "js":["jquery.min.js", "picwords.js"],
    "run_at": "document_end"
  }]
}
